<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <parent>
    <groupId>net.jangaroo</groupId>
    <artifactId>jangaroo-libs</artifactId>
    <version>0.5.28</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <groupId>net.jangaroo</groupId>
  <artifactId>joounit</artifactId>
  <packaging>jangaroo</packaging>
  <version>0.5.28</version>
  
  <name>joounit</name>
  <description>JooUnit - Jangaroo FlexUnit 3</description>
  
  <url>TODO</url>

  <developers>

    <developer>
      <id>fwienber</id>
      <name>Frank Wienberg</name>
      <email>frank@jangaroo.net</email>
      <timezone>+1</timezone>
    </developer>

  </developers>

  <repositories>
    <repository>
      <id>sonatype</id>
      <name>sonatype repository</name>
      <url> http://repository.sonatype.org/content/groups/flexgroup</url>
    </repository>
  </repositories>
  <profiles>
    <profile>
      <id>integrationtest</id>
      <activation>
        <activeByDefault>true</activeByDefault>
      </activation>
    </profile>
  </profiles>


  <build>


    <plugins>
      <plugin>
        <groupId>net.jangaroo</groupId>
        <artifactId>jangaroo-maven-plugin</artifactId>
        <configuration>
          <enableGuessingTypeCasts>true</enableGuessingTypeCasts>
          <jooUnitTestExecutionTimeout>120000</jooUnitTestExecutionTimeout>
        </configuration>
      </plugin>
    </plugins>
  </build>

  <dependencies>

    <dependency>
      <groupId>net.jangaroo</groupId>
      <artifactId>jooflash</artifactId>
      <version>0.5.28</version>
      <type>jangaroo</type>
    </dependency>

    <dependency>
      <groupId>net.jangaroo</groupId>
      <artifactId>jooflexframework</artifactId>
      <version>0.5.28</version>
      <type>jangaroo</type>
    </dependency>

  </dependencies>
</project>
